{
    "name": "YT",
    "version": "1.0.0",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": {
            "19": "button/19.png",
            "38": "button/38.png"
        },
        "default_title": "YT"
    },
    "background": {
        "page": "bgpage.html"
    },
    "permissions": [
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "webRequestFilterResponse.serviceWorkerScript",
        "downloads",
        "https://*/*"
    ]
}